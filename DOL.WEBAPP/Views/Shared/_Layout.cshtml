    <script type="text/javascript">

        function OnSaveSuccessReload(message) {

                Swal.fire({
                icon: 'success',
                html: message,
                type: 'success',
                buttonsStyling: true,
                showConfirmButton: true,
                confirmButtonColor: '#3085d6',
                confirmButtonClass: 'btn-flat shadow btn btn-success',
                confirmButtonText: 'ตกลง',
                allowOutsideClick: false,
            }).then((result) => {
                if (result.isConfirmed) {
                    // Redirect to another page
                    window.location.reload();
                }
            });

        }

        function OnSaveSuccess(message) {
            Swal.fire({
                icon: 'success',
                html: message,
                type: 'success',
                buttonsStyling: true,
                showConfirmButton: true,
                confirmButtonColor: '#3085d6',
                confirmButtonClass: 'btn-flat shadow btn btn-success',
                confirmButtonText: 'ตกลง',

            })
        }


        function OnSaveFinish(message) {

            Swal.fire({
                icon: 'success',
                html: message,
                type: 'success',
                buttonsStyling: true,
                showConfirmButton: true,
                confirmButtonColor: '#3085d6',
                confirmButtonClass: 'btn-flat shadow btn btn-success',
                confirmButtonText: 'ตกลง',
            }).then((result) => {
                if (result.isConfirmed) {
                    // Redirect to another page
                    window.location.href = '@Url.Action("Index", "Home")'; // Replace with your desired URL
                }
            });
        }

        function OnSaveError(message) {
            Swal.fire({
                icon: 'error',
                html: message,
                type: 'error',
                buttonsStyling: true,

                //sweet alert for "Cancel" Buttom
                showCancelButton: true,
                cancelButtonColor: '#d33',
                cancelButtonClass: 'btn-flat shadow btn btn-danger ml-1',
                cancelButtonText: 'ลองใหม่อีกครั้ง !',

                //sweet alert for "Confirm" Buttom
                showConfirmButton: false,
                confirmButtonColor: '#3085d6',
                confirmButtonClass: 'btn-flat shadow btn btn-success',
                confirmButtonText: 'ตกลง',

            })
        }

        function OnSaveWarning(message) {
            Swal.fire({
                icon: 'warning',
                html: message,
                type: 'warning',
                buttonsStyling: true,

                //sweet alert for "Cancel" Buttom
                showCancelButton: true,
                cancelButtonColor: '#d33',
                cancelButtonClass: 'btn-flat shadow btn btn-danger ml-1',
                cancelButtonText: 'ลองใหม่อีกครั้ง !',

                //sweet alert for "Confirm" Buttom
                showConfirmButton: false,
                confirmButtonColor: '#3085d6',
                confirmButtonClass: 'btn-flat shadow btn btn-success',
                confirmButtonText: 'ตกลง',

            })
        }</script>

